  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "npm run dev --prefix nftfun-monorepo-frontend",
    "dev:server": "npm run dev --prefix nftfun-monorepo-backend",
    "generate": "run-p generate:*",
    "generate:client": "npm run generate --prefix nftfun-monorepo-frontend",
    "generate:server": "npm run generate --prefix nftfun-monorepo-backend",
    "test": "run-p test:*",
    "test:client": "npm run test --prefix nftfun-monorepo-frontend",
    "test:server": "npm run test --prefix nftfun-monorepo-backend",
    "typecheck": "run-p typecheck:*",
    "typecheck:client": "npm run typecheck --prefix nftfun-monorepo-frontend",
    "typecheck:server": "npm run typecheck --prefix nftfun-monorepo-backend"
  },
  "author": "",
