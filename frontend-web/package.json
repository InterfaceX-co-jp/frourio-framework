{
  "name": "frontend-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --prefix-colors=\"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" --kill-others-on-fail \"npm run dev:next\" \"npm run dev:aspida\" \"npm run dev:path\"",
    "dev:next": "next dev",
    "dev:aspida": "aspida --watch --config aspida.config.cjs",
    "dev:path": "pathpida --enableStatic --ignorePath .gitignore --watch",
    "build": "concurrently --prefix-colors=\"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" --kill-others-on-fail \"npm run build:path\" \"npm run build:aspida\" \"npm run build:next\"",
    "build:path": "npm run generate:path",
    "build:aspida": "npm run generate:aspida",
    "build:next": "next build",
    "generate": "concurrently --prefix-colors=\"bgBlue.bold,bgMagenta.bold\" --kill-others-on-fail \"npm run generate:path\" \"npm run generate:aspida\"",
    "generate:path": "pathpida --enableStatic --ignorePath .gitignore",
    "generate:aspida": "aspida --config aspida.config.cjs",
    "test": "vitest run --pool=forks",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aspida/axios": "^1.14.0",
    "@aspida/swr": "^1.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@supabase/supabase-js": "^2.50.0",
    "aspida": "^1.14.0",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "commonConstantsWithClient": "file:../backend-api/commonConstantsWithClient",
    "commonTypesWithClient": "file:../backend-api/commonTypesWithClient",
    "dayjs": "^1.11.13",
    "flowbite": "^2.5.2",
    "flowbite-react": "^0.10.2",
    "jotai": "^2.12.5",
    "js-cookie": "^3.0.5",
    "next": "15.3.4",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-select": "^5.10.1",
    "react-spinners": "^0.17.0",
    "react-toastify": "^11.0.5",
    "swr": "^2.3.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.61"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.3.3",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "concurrently": "^9.1.0",
    "pathpida": "^0.25.0",
    "postcss": "^8.5.4",
    "supabase": "^2.24.3",
    "tailwindcss": "^3.4.15",
    "typescript": "^5",
    "typescript-eslint": "^8.34.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  }
}